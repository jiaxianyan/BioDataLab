for g in genes_std if g in gene_to_value]\n    if expr_values:\n        score = float(sum(expr_values) / len(expr_values))\n    else:\n        score = 0.0\n    scores[ct] = score\n    details[ct] = {\n        \"n_marker_genes\": len(genes_std),\n        \"n_matched_genes\": len(expr_values),\n        \"score_mean_expr\": score\n    }\n\nscores_path = os.path.join(tmp_dir, \"cell_1_celltype_scores.json\")\nwith open(scores_path, \"w\") as f:\n    json.dump({\"scores\": scores, \"details\": details}, f, indent=2)\nprint(f\"Saved cell type scores for cell_1 to: {scores_path}\")\n\nprint(\"Scores:\")\nfor ct, sc in sorted(scores.items(), key=lambda x: -x[1]):\n    print(f\"{ct}: {sc:.4f} (matched {details[ct]['n_matched_genes']} genes)\")\n\n# Choose best type; if all scores are zero or no matches, set as Unassigned\nbest_type = max(scores.items(), key=lambda x: x[1])[0] if scores else \"Unassigned\"\nif scores and scores[best_type] == 0.0:\n    best_type = \"Unassigned\"\n\nprint(f\"\\nSelected annotation for cell_1: {best_type}\")\n\nprint(\"\\nUpdated plan:\")\nprint(\"1. [\u2713] Load CellMarker.csv and cell_1.txt\")\nprint(\"2. [\u2713] Process CellMarker markers into gene sets per cell type\")\nprint(\"3. [\u2713] Process cell_1 expression into a gene \u2192 value mapping\")\nprint(\"4. [\u2713] Score each requested cell type for cell_1 and pick best match\")\nprint(\"5. [ ] Save final annotation JSON and intermediate outputs to required paths\")\n\n# ---------- Step 5: Save final annotation JSON ----------\n\nprint(\"\\nExecuting Step 5: Save final annotation to required path\")\n\noutput_dir = \"/root/biodatalab/log/biomni_v20260209_pred_results/OpenAI/Azure-GPT-5.1\"\nos.makedirs(output_dir, exist_ok=True)\noutput_path = os.path.join(output_dir, \"cancerscem_annotate.json\")\n\nfinal_result = {\"cell_1\": best_type}\nwith open(output_path, \"w\") as f:\n    json.dump(final_result, f, indent=2)\n\nprint(f\"Saved final annotation JSON to: {output_path}\")\nprint(