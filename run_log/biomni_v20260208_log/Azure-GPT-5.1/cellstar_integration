f norm_key(x: str) -> str:\n    \"\"\"Lowercase, strip, and collapse internal whitespace to create a stable lookup key.\"\"\"\n    if x is None:\n        return \"\"\n    return \" \".join(x.strip().lower().split())\n\n# Taxonomy: map to common English names that are standardly associated with NCBI taxonomy\nspecies_map = {\n    # Human\n    \"human\": \"human\",\n    \"homo sapiens\": \"human\",\n    # Mouse\n    \"mouse\": \"house mouse\",\n    \"mus musculus\": \"house mouse\",\n    \"mouse (c57bl/6)\": \"house mouse\",\n    \"m. musculus\": \"house mouse\",\n    # Rat\n    \"rat\": \"norway rat\",\n    \"rattus norvegicus\": \"norway rat\",\n    # Zebrafish\n    \"zebrafish\": \"zebrafish\",\n    \"danio rerio\": \"zebrafish\",\n    # Drosophila\n    \"drosophila melanogaster\": \"fruit fly\",\n    \"fruit fly\": \"fruit fly\",\n    # Axolotl\n    \"axolotl\": \"axolotl\",\n    \"ambystoma mexicanum\": \"axolotl\",\n    # African malaria mosquito\n    \"anopheles gambiae\": \"african malaria mosquito\",\n    \"african malaria mosquito\": \"african malaria mosquito\",\n}\n\n# Uberon\u2011aligned tissue labels (plain text names)\ntissue_map = {\n    \"bladder\": \"bladder\",\n    \"urinary bladder\": \"bladder\",\n    \"haemolymph\": \"haemolymph\",\n    \"hemolymph\": \"haemolymph\",\n    \"blood\": \"blood\",\n    \"peripheral blood\": \"peripheral blood\",\n    \"bone marrow\": \"bone marrow\",\n    \"brain\": \"brain\",\n    \"cerebral cortex\": \"cerebral cortex\",\n    \"heart\": \"heart\",\n    \"lung\": \"lung\",\n    \"liver\": \"liver\",\n    \"kidney\": \"kidney\",\n    \"spleen\": \"spleen\",\n    \"intestine\": \"intestine\",\n    \"small intestine\": \"small intestine\",\n    \"colon\": \"colon\",\n    \"skin\": \"skin\",\n    \"pancreas\": \"pancreas\",\n    \"blood vessel\": \"blood vessel\",\n    \"lymph node\": \"lymph node\",\n    \"thymus\": \"thymus\",\n    \"testis\": \"testis\",\n    \"ovary\": \"ovary\",\n    \"retina\": \"retina\",\n    \"