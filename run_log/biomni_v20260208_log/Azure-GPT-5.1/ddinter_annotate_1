sk_levels = []\nrationales = []\n\nfor i, desc in enumerate(ddi_descriptions, start=1):\n    level = classify_ddi(desc)\n    risk_levels.append(level)\n    rationales.append({\n        \"index\": i,\n        \"description\": desc,\n        \"assigned_level\": level\n    })\n\nprint(\"\\nAssigned risk levels:\")\nfor i, (d, r) in enumerate(zip(ddi_descriptions, risk_levels), start=1):\n    print(f\"DDI {i}: level={r} | text={d}\")\n\n# Step 3: Save intermediate classification data\nintermediate_path = os.path.join(tmp_dir, \"classification_details.json\")\nwith open(intermediate_path, \"w\") as f:\n    json.dump(rationales, f, indent=2)\n\nprint(f\"\\nIntermediate classification details saved to: {intermediate_path}\")\n\n# Update plan: mark step 2 and 3 as complete\nplan_lines[1] = \"2. [\u2713] Classify each DDI into risk levels (Major, Moderate, Minor, Unknown) based on textual description\"\nplan_lines[2] = \"3. [\u2713] Save intermediate classification data into the tmp directory\"\nwith open(plan_path, \"w\") as f:\n    f.write(\"\\n\".join(plan_lines) + \"\\n\")\n\nprint(\"\\nPlan after steps 2 and 3:\")\nprint(open(plan_path).read())\n\n# Step 4: Save final JSON list to required path\nfinal_output_path = os.path.join(pred_dir, \"ddinter_annotate_1.json\")\nwith open(final_output_path, \"w\") as f:\n    json.dump(risk_levels, f)\n\n# Mark step 4 complete\nplan_lines[3] = \"4. [\u2713] Write final risk level list JSON to /root/biodatalab/log/biomni_v20260209_pred_results/OpenAI/Azure-GPT-5.1/ddinter_annotate_1.json\"\nwith open(plan_path, \"w\") as f:\n    f.write(\"\\n\".join(plan_lines) + \"\\n\")\n\nprint(f\"\\nFinal risk level list saved to: {final_output_path}\")\nprint(\"Final risk levels list (in required format):\")\nprint(json.dumps(risk_levels))\n\nprint(\"\\nFinal plan state:\")\nprint(open(plan_path).read())\n</execute>"
