ts(v)\n\nextract_traits(data)\n\n# Deduplicate while preserving order\nseen = set()\nunique_traits = []\nfor t in traits:\n    if t not in seen:\n        seen.add(t)\n        unique_traits.append(t)\n\nprint(f\"Total raw strings collected: {len(traits)}\")\nprint(f\"Unique trait candidates: {len(unique_traits)}\")\n\n# Save raw unique traits snapshot\nraw_traits_path = tmp_dir / \"step1_raw_unique_traits.json\"\nwith open(raw_traits_path, \"w\", encoding=\"utf-8\") as f:\n    json.dump(unique_traits, f, indent=2, ensure_ascii=False)\nprint(f\"Saved raw unique traits to {raw_traits_path}\")\n\n# Update plan after Step 1\nprint(\"\\nUpdated plan:\")\nplan[0] = \"[\u2713] 1. Inspect reported_trails.json to load the list of original traits.\"\nfor p in plan:\n    print(p)\n\n# =========================\n# Step 2: Design standardization function\n# =========================\nprint(\"\\n[STEP 2] Designing standardization function...\")\n\n# Abbreviation / wording normalization dictionary.\n# This is generic and may be refined if needed.\nNORMALIZATION_MAP = {\n    r\"\\bU\\.?S\\.?A\\.?\\b\": \"USA\",\n    r\"\\bU\\.?K\\.?\\b\": \"UK\",\n    r\"\\bSNPs?\\b\": \"single nucleotide polymorphism\",\n    r\"\\bHR\\b\": \"hazard ratio\",\n    r\"\\bOR\\b\": \"odds ratio\",\n    r\"\\bCI\\b\": \"confidence interval\",\n    r\"\\bBMI\\b\": \"body mass index\",\n    r\"\\bBP\\b\": \"blood pressure\",\n    r\"\\bHbA1c\\b\": \"hemoglobin A1c\",\n    r\"\\bGWAS\\b\": \"genome-wide association study\",\n    r\"\\bP\\s*value\\b\": \"p-value\",\n    r\"\\bp value\\b\": \"p-value\",\n}\n\ndef normalize_language(text: str) -> str:\n    \"\"\"Apply regex-based normalization for common abbreviations and phrases.\"\"\"\n    normalized = text\n    for pattern, replacement in NORMALIZATION_MAP.items():\n        normalized = re.sub(pattern, replacement, normalized, flags=re.IGNORECASE)\n    return normalized\n\ndef standardize_trait(trait: str) -> str:\n    \"\"\"Standardize a trait string: strip noise, normalize language.\"\"\"\n    if trait is None:\n        return \"\"\n    s = str(trait)\n\n    # 1) Remove enclosing quotes or unmatched quotation marks commonly used in text\n    s = s.strip()\n\n    # 2) Remove leading/trailing punctuation like ':', ';', ',', '.', '*'\n    s = re.sub(r'^[\\s\\-\\:\\;\\,\\.\\*\\[\\]\\(\\)]+', '', s)\n    s = re.sub(r'[\\s\\-\\:\\;\\,\\.\\*\\[\\]\\(\\)]+$', '', s)\n\n    # 3) Remove publication-specific artifacts: figure/table refs, supplementary refs\n    # e.g., \"(Fig. 1)\", \"[Table 2]\", \"Figure 3\", \"Table S1\"\n    s = re.sub(r'\\b(Fig(?:ure)?|Table|Supplementary Figure|Supplementary Table)\\s*[S\\dIVXivx]+\\b', '', s)\n    s = re.sub(r'\\((Fig(?:ure)?|Table)\\s*[S\\dIVXivx]+\\)', '', s)\n    s = re.sub(r'\\[Fig(?:ure)?\\s*[S\\dIVXivx]+\\]', '', s)\n    s = re.sub(r'\\[Table\\s*[S\\dIVXivx]+\\]', '', s)\n\n    # 4) Remove trailing reference markers like [1], [23], (1), {1}\n    s = re.sub(r'\\s*\\[[0-9,\\-\\s]+\\]\\s*$', '', s)\n    s = re.sub(r'\\s*\\([0-9,\\-\\s]+\\)\\s*$', '', s)\n    s = re.sub(r'\\s*\\{[0-9,\\-\\s]+\\}\\s*$', '', s)\n\n    # 5) Replace multiple whitespaces with single and normalize dashes\n    s = s.replace(\"\u2013\", \"-\").replace(\"\u2014\", \"-\")\n    s = re.sub(r'\\s+', ' ', s)\n\n    # 6) Normalize common ambiguous wording: remove \"the\", \"a\", \"an\" at ends, generic phrases\n    # but keep inner 'the' etc.\n    s = s.strip(\" -\")\n    # Lowercase for further linguistic normalization\n    lower = s.lower()\n\n    # 7) Remove leading ambiguous phrases like \"association of\", \"effect of\", \"impact of\"\n    ambiguous_starts = [\n        \"association of \", \"associations of \", \"effect of \", \"effects of \",\n        \"impact of \", \"impacts of \", \"role of \", \"roles of \",\n        \"relationship between \", \"relationships between \",\n        \"correlation between \", \"study of \", \"analysis of \",\n    ]\n    for prefix in ambiguous_starts:\n        if lower.startswith(prefix):\n            s = s[len(prefix):]\n            lower = s.lower()\n            break\n\n    # 8) Remove trailing vague qualifiers that don't define the trait itself\n    ambiguous_ends = [\n        \" in humans\", \" in human\", \" in patients\", \" in subjects\",\n        \" in mice\", \" in mouse\", \" in rats\", \" in controls\",\n        \" in cases\", \" in individuals\", \" in participants\",\n        \" levels\", \" level\", \" measurement\", \" measurements\",\n        \" value\", \" values\", \" rate\", \" rates\",\n    ]\n    for suffix in ambiguous_ends:\n        if lower.endswith(suffix):\n            s = s[: -len(suffix)]\n            lower = s.lower()\n            break\n\n    # 9) Apply language normalization (abbreviations, etc.)\n    s = normalize_language(s)\n\n    # 10) Final trim and collapse spaces\n    s = s.strip()\n    s = re.sub(r'\\s+', ' ', s)\n\n    # If it becomes empty, fallback to original trimmed\n    if not s:\n        s = trait.strip() if isinstance(trait, str) else str(trait)\n    return s\n\n# Test the function on first few traits and log\npreview = []\nfor t in unique_traits[:20]:\n    preview.append({\"original\": t, \"standardized\": standardize_trait(t)})\n\npreview_path = tmp_dir / \"step2_preview_standardization_examples.json\"\nwith open(preview_path, \"w\", encoding=\"utf-8\") as f:\n    json.dump(preview, f, indent=2, ensure_ascii=False)\nprint(f\"Saved preview of standardization examples to {preview_path}\")\n\n# Update plan after Step 2\nprint(\"\\nUpdated plan:\")\nplan[1] = \"[\u2713] 2. Design and implement a standardization function for trait strings (remove publication-specific symbols, normalize wording).\"\nfor p in plan:\n    print(p)\n\n# =========================\n# Step 3: Apply standardization to all traits\n# =========================\nprint(\"\\n[STEP 3] Applying standardization to all traits and building mapping dict...\")\n\nmapping = {}\nfor t in unique_traits:\n    std = standardize_trait(t)\n    mapping[t] = std\n\n# Save intermediate full mapping snapshot\nintermediate_mapping_path = tmp_dir / \"step3_full_mapping_raw.json\"\nwith open(intermediate_mapping_path, \"w\", encoding=\"utf-8\") as f:\n    json.dump(mapping, f, indent=2, ensure_ascii=False)\nprint(f\"Saved intermediate full mapping to {intermediate_mapping_path}\")\n\n# Also log set of standardized traits for inspection\nstandardized_traits = sorted(set(mapping.values()))\nstandardized_traits_path = tmp_dir / \"step3_unique_standardized_traits.json\"\nwith open(standardized_traits_path, \"w\", encoding=\"utf-8\") as f:\n    json.dump(standardized_traits, f, indent=2, ensure_ascii=False)\nprint(f\"Saved unique standardized traits to {standardized_traits_path}\")\n\n# Update plan after Step 3\nprint(\"\\nUpdated plan:\")\nplan[2] = \"[\u2713] 3. Apply the standardization function to all traits, build mapping dict {original: standardized}, and log intermediate artifacts.\"\nfor p in plan:\n    print(p)\n\n# =========================\n# Step 4: Save final mapping dict to specified output path\n# =========================\nprint(\"\\n[STEP 4] Saving final mapping dict to requested output path...\")\nwith open(final_out_path, \"w\", encoding=\"utf-8\") as f:\n    json.dump(mapping, f, indent=2, ensure_ascii=False)\nprint(f\"Final mapping dict saved to {final_out_path}\")\n\n# Final plan update\nprint(\"\\nFinal plan status:\")\nplan[3] = \"[\u2713] 4. Save final mapping dict to the specified output JSON path.\"\nfor p in plan:\n    print(p)\n\n</execute>"
