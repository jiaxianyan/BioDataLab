r (name)\n        r\"([^)]+?)\\s*\\(([A-Za-z0-9\\-\\(\\)]{2,})\\)\"   # name (abbr)\n    ]\n    \n    # Search in first 2000 characters\n    search_text = text[:2000]\n    \n    for pattern in patterns:\n        matches = re.findall(pattern, search_text)\n        if matches:\n            for match in matches:\n                part1, part2 = match[0], match[1]\n                # Check if part1 looks like an abbreviation (shorter, more uppercase)\n                if (len(part1) <= 10 and any(c.isupper() for c in part1) and \n                    len(part2) > 5 and any(c.islower() for c in part2)):\n                    if pattern == patterns[0]:  # abbr (name)\n                        ncAA_abbr = part1\n                        ncAA_name = part2\n                    else:  # name (abbr)\n                        ncAA_abbr = part2\n                        ncAA_name = part1\n                    break\n            if ncAA_abbr != \"not available\":\n                break\n    \n    return ncAA_abbr, ncAA_name\n\ndef parse_aaRS_info(text):\n    \"\"\"Extract aaRS ID and origin organism\"\"\"\n    aaRS_id = \"not available\"\n    aaRS_origin = \"not available\"\n    \n    # Organism abbreviation mapping\n    org_map = {\n        \"Ec\": \"Escherichia coli\",\n        \"Mj\": \"Methanocaldococcus jannaschii\", \n        \"Mb\": \"Methanosarcina barkeri\",\n        \"Bs\": \"Bacillus subtilis\",\n        \"Sc\": \"Saccharomyces cerevisiae\",\n        \"Mm\": \"Methanosarcina mazei\",\n        \"Pf\": \"Pyrococcus furiosus\"\n    }\n    \n    # Look for aaRS patterns like \"Ec-TyrRS\" or \"MjTyrRS\"\n    aaRS_patterns = [\n        r\"([A-Z][a-z]?)-?([A-Za-z]{3,})RS\",  # Ec-TyrRS or EcTyrRS\n        r\"([A-Z][a-z]?)\\s*([A-Za-z]{3,})\\s*RS\"  # Ec Tyr RS\n    ]\n    \n    search_text = text[:3000]  # Search in first 3000 chars\n    \n    for pattern in aaRS_patterns:\n        matches = re.findall(pattern, search_text)\n        if matches:\n            org_abbr, substrate = matches[0]\n            # Clean up substrate (should be 3-letter amino acid code)\n            substrate = re.sub(r'[^A-Za-z]', '', substrate)\n            if len(substrate) >= 3:\n                substrate = substrate[:3].capitalize()\n                aaRS_id = f\"{org_abbr}-{substrate}RS\"\n                \n                # Check for mutations in surrounding text\n                mutation_context = text[max(0, search_text.find(org_abbr)-100):search_text.find(org_abbr)+200]\n                mutation_pattern = r\"\\b([A-Z]\\d+[A-Z])\\b\"\n                mutations = re.findall(mutation_pattern, mutation_context)\n                if mutations:\n                    # Remove duplicates and sort\n                    unique_mutations = sorted(list(set(mutations)))\n                    aaRS_id += f\" ({', '.join(unique_mutations)})\"\n                else:\n                    aaRS_id += \" (wild type)\"\n                \n                if org_abbr in org_map:\n                    aaRS_origin = org_map[org_abbr]\n                break\n            if aaRS_id != \"not available\":\n                break\n    \n    return aaRS_id, aaRS_origin\n\ndef parse_tRNA_info(text):\n    \"\"\"Extract tRNA ID and origin organism\"\"\"\n    tRNA_id = \"not available\"\n    tRNA_origin = \"not available\"\n    \n    org_map = {\n        \"Ec\": \"Escherichia coli\",\n        \"Mj\": \"Methanocaldococcus jannaschii\", \n        \"Mb\": \"Methanosarcina barkeri\",\n        \"Bs\": \"Bacillus subtilis\",\n        \"Sc\": \"Saccharomyces cerevisiae\",\n        \"Mm\": \"Methanosarcina mazei\",\n        \"Pf\": \"Pyrococcus furiosus\"\n    }\n    \n    # Look for tRNA patterns like \"Ec-tRNA^Tyr_CUA\" or \"Ec-tRNA-Tyr-CUA\"\n    tRNA_patterns = [\n        r\"([A-Z][a-z]?)-tRNA\\^?([A-Za-z]{3})_?([AUCG]{3})\",\n        r\"([A-Z][a-z]?)\\s*tRNA\\s*([A-Za-z]{3})\\s*([AUCG]{3})\",\n        r\"([A-Z][a-z]?)-tRNA\\s*([A-Za-z]{3})\\s*([AUCG]{3})\"\n    ]\n    \n    search_text = text[:3000]\n    \n    for pattern in tRNA_patterns:\n        matches = re.findall(pattern, search_text)\n        if matches:\n            org_abbr, aa, anticodon = matches[0]\n            tRNA_id = f\"{org_abbr}-tRNA {aa} {anticodon}\"\n            if org_abbr in org_map:\n                tRNA_origin = org_map[org_abbr]\n            break\n    \n    return tRNA_id, tRNA_origin\n\ndef parse_protein_info(text):\n    \"\"\"Extract protein, position, and host information\"\"\"\n    protein = \"not available\"\n    position = \"not available\"\n    host = \"not available\"\n    \n    # Common protein names to look for\n    protein_keywords = [\n        (\"green fluorescent protein\", \"GFP\"),\n        (\"superoxide dismutase\", \"SOD\"), \n        (\"dihydrofolate reductase\", \"DHFR\"),\n        (\"ubiquitin\", \"ubiquitin\"),\n        (\"calmodulin\", \"calmodulin\"),\n        (\"lysozyme\", \"lysozyme\"),\n        (\"myoglobin\", \"myoglobin\"),\n        (\"cytochrome\", \"cytochrome\")\n    ]\n    \n    search_text = text[:5000]\n    \n    # Find protein\n    for full_name, abbr in protein_keywords:\n        if full_name.lower() in search_text.lower():\n            protein = full_name\n            break\n        elif abbr.lower() in search_text.lower() and len(abbr) > 2:\n            protein = abbr\n            break\n    \n    # Find positions - look for patterns like \"position 33\", \"site 123\", etc.\n    pos_patterns = [\n        r\"position\\s+(\\d+)\",\n        r\"site\\s+(\\d+)\",\n        r\"residue\\s+(\\d+)\",\n        r\"(\\d+)\\s+(?:position|site|residue)\"\n    ]\n    \n    for pattern in pos_patterns:\n        matches = re.findall(pattern, search_text, re.IGNORECASE)\n        if matches:\n            position = matches[0]\n            break\n    \n    # Find host organisms\n    host_patterns = [\n        (\"Escherichia coli\", [r\"E\\.?\\s*coli\", r\"Escherichia coli\"]),\n        (\"Saccharomyces cerevisiae\", [r\"S\\.?\\s*cerevisiae\", r\"yeast\"]),\n        (\"in vitro\", [r\"in\\s+vitro\"]),\n        (\"mammalian cells\", [r\"mammalian\"]),\n        (\"HEK293\", [r\"HEK293\"])\n    ]\n    \n    for host_name, patterns in host_patterns:\n        for pattern in patterns:\n            if re.search(pattern, search_text, re.IGNORECASE):\n                host = host_name\n                break\n        if host != \"not available\":\n            break\n    \n    return protein, position, host\n\n# Process each target file\nfor filename in target_files:\n    pdf_path = os.path.join(input_dir, filename)\n    if os.path.exists(pdf_path):\n        print(f\"Processing {filename}...\")\n        \n        # Extract text\n        text = extract_text_from_pdf(pdf_path)\n        intermediate_path = os.path.join(intermediate_dir, filename.replace('.pdf', '.txt'))\n        with open(intermediate_path, 'w', encoding='utf-8') as f:\n            f.write(text[:5000])  # Save first 5000 chars\n        \n        # Parse information\n        ncAA_abbr, ncAA_name = parse_ncAA_info(text)\n        aaRS_id, aaRS_origin = parse_aaRS_info(text)\n        tRNA_id, tRNA_origin = parse_tRNA_info(text)\n        protein, position, host = parse_protein_info(text)\n        \n        # Create CSV row\n        csv_row = f\"{ncAA_abbr};{ncAA_name};{aaRS_id};{aaRS_origin};{tRNA_id};{tRNA_origin};{protein};{position};{host}\"\n        \n        # Save CSV\n        csv_filename = filename.replace('.pdf', '.csv')\n        csv_path = os.path.join(output_dir, csv_filename)\n        with open(csv_path, 'w', encoding='utf-8') as f:\n            header = \"ncAA abbreviation(s) used in the publication;ncAA name, as mentioned in the publication;aaRS ID (abbr. organism, abbr. natural substrate, RS, mutations if any);aaRS origin organism full name;tRNA ID (abbr. organism, tRNA, natural AA transported, anticodon);tRNA organism;Tested in (protein);Tested in (protein position);Tested in (organism/in vitro)\"\n            f.write(header + \"\\n\")\n            f.write(csv_row + \"\\n\")\n        \n        print(f\"Completed {filename}:\")\n        print(f\"  ncAA: {ncAA_abbr} / {ncAA_name}\")\n        print(f\"  aaRS: {aaRS_id} / {aaRS_origin}\")\n        print(f\"  tRNA: {tRNA_id} / {tRNA_origin}\")\n        print(f\"  Protein: {protein} at {position} in {host}\")\n        print()\n    else:\n        print(f\"File {filename} not found!\")\n</execute>"
