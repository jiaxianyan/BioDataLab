erns:\n        for pattern in patterns:\n            if re.search(pattern, text, re.IGNORECASE):\n                host = host_name\n                break\n        if host != \"not available\":\n            break\n    \n    return protein, position, host\n\n# Process each target file\nfor filename in target_files:\n    pdf_path = os.path.join(input_dir, filename)\n    if os.path.exists(pdf_path):\n        print(f\"Processing {filename}...\")\n        \n        # Extract text\n        text = extract_text_from_pdf(pdf_path)\n        intermediate_path = os.path.join(intermediate_dir, filename.replace('.pdf', '.txt'))\n        with open(intermediate_path, 'w') as f:\n            f.write(text[:5000])  # Save first 5000 chars as intermediate\n        \n        # Parse information\n        ncAA_abbr, ncAA_name = parse_ncAA_info(text)\n        aaRS_id, aaRS_origin = parse_aaRS_info(text)\n        tRNA_id, tRNA_origin = parse_tRNA_info(text)\n        protein, position, host = parse_protein_info(text)\n        \n        # Create CSV row\n        csv_row = f\"{ncAA_abbr};{ncAA_name};{aaRS_id};{aaRS_origin};{tRNA_id};{tRNA_origin};{protein};{position};{host}\"\n        \n        # Save CSV\n        csv_filename = filename.replace('.pdf', '.csv')\n        csv_path = os.path.join(output_dir, csv_filename)\n        with open(csv_path, 'w') as f:\n            f.write(\"ncAA abbreviation(s) used in the publication;ncAA name, as mentioned in the publication;aaRS ID (abbr. organism, abbr. natural substrate, RS, mutations if any);aaRS origin organism full name;tRNA ID (abbr. organism, tRNA, natural AA transported, anticodon);tRNA organism;Tested in (protein);Tested in (protein position);Tested in (organism/in vitro)\\n\")\n            f.write(csv_row + \"\\n\")\n        \n        print(f\"Completed {filename}\")\n    else:\n        print(f\"File {filename} not found!\")\n</execute>"
