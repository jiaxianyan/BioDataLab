cs.append(auroc)\n\n# Create results dataframe\nresults_df = pd.DataFrame({\n    'gene': genes,\n    'p_value': p_values,\n    'auroc': aurocs\n})\n\n# Save final results\noutput_path = \"/root/autodl-tmp/biomni_v20260208_pred_results/deepseek-v3.2-fast/clinicalomicsdb_annotate.csv\"\nresults_df.to_csv(output_path, index=False)\nprint(f\"Saved results to: {output_path}\")\n\n# Save intermediate results\ngene_data_path = os.path.join(intermediate_dir, \"gene_expression_data.csv\")\ngene_data.to_csv(gene_data_path, index=False)\n\ngroup_info = pd.DataFrame({\n    'sample': sample_names,\n    'group': group_labels\n})\ngroup_info_path = os.path.join(intermediate_dir, \"group_information.csv\")\ngroup_info.to_csv(group_info_path, index=False)\n\ndetailed_path = os.path.join(intermediate_dir, \"detailed_results.csv\")\nresults_df.to_csv(detailed_path, index=False)\n\n# Print summary\nprint(f\"\\nAnalysis completed successfully!\")\nprint(f\"Number of genes analyzed: {len(results_df)}\")\nprint(f\"Number of significant genes (p < 0.05): {sum(results_df['p_value'] < 0.05)}\")\nprint(f\"Number of genes with AUROC > 0.8: {sum(results_df['auroc'] > 0.8)}\")\n\nprint(f\"\\nFirst 10 rows of results:\")\nprint(results_df.head(10))\n\nprint(f\"\\nFiles saved:\")\nprint(f\"1. Final results: {output_path}\")\nprint(f\"2. Intermediate gene data: {gene_data_path}\")\nprint(f\"3. Intermediate group info: {group_info_path}\")\nprint(f\"4. Intermediate detailed results: {detailed_path}\")\n</execute>"
