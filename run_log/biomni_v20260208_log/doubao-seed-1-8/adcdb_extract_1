 with XXX payload\"\n    payload_match = re.search(r'\\bpayload\\s+(was|is|loaded with)\\s+([A-Za-z0-9-]+)\\b', full_text, re.IGNORECASE)\n    if not payload_match:\n        payload_match = re.search(r'\\b([A-Za-z0-9-]+)\\s+payload\\b', full_text, re.IGNORECASE)\n    if payload_match:\n        adc_info[\"Payload\"] = payload_match.group(2) if payload_match.group(2) else payload_match.group(1)\n    \n    # Extract Conjugate Type - look for patterns like \"conjugated via XXX\" or \"using XXX conjugation\"\n    conjugate_type_match = re.search(r'\\bconjugated\\s+via\\s+([A-Za-z0-9-]+)\\b', full_text, re.IGNORECASE)\n    if not conjugate_type_match:\n        conjugate_type_match = re.search(r'\\b([A-Za-z0-9-]+)\\s+conjugation\\b', full_text, re.IGNORECASE)\n    if conjugate_type_match:\n        adc_info[\"Conjugate Type\"] = conjugate_type_match.group(1)\n    \n    # Extract DAR - look for patterns like \"DAR of X.X\" or \"DAR = X.X\"\n    dar_match = re.search(r'\\bDAR\\s+(of|=)\\s+(\\d+\\.?\\d*)', full_text, re.IGNORECASE)\n    if dar_match:\n        adc_info[\"DAR\"] = dar_match.group(2)\n    \n    # Extract Drug Status - look for patterns like \"is in Phase X\" or \"currently in Phase X\"\n    status_match = re.search(r'\\b(is|currently)\\s+in\\s+(Phase\\s*\\d+|Preclinical)\\b', full_text, re.IGNORECASE)\n    if status_match:\n        adc_info[\"Drug Status\"] = status_match.group(2)\n    \n    return adc_info\n\n# Process each PDF\npdf_files = [\n    (\"benchmark/dataset/ADCdb/ref2.pdf\", \"ref2\"),\n    (\"benchmark/dataset/ADCdb/ref4.pdf\", \"ref4\"),\n    (\"benchmark/dataset/ADCdb/ref5.pdf\", \"ref5\")\n]\n\nadc_records = []\nfor pdf_path, paper_id in pdf_files:\n    print(f\"Processing {paper_id}...\")\n    adc_info = extract_adc_info(pdf_path, paper_id)\n    adc_records.append(adc_info)\n    print(f\"Extracted info for {paper_id}: {adc_info}\")\n\n# Save to CSV\noutput_path = \"/root/autodl-tmp/biomni_v20260208_pred_results/doubao-seed-1-8/adcdb_extract_1.csv