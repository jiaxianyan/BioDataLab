            "content": "<execute> #!R\nlibrary(GEOquery)\nlibrary(tidyverse)\n\n# Create output directory if not exists\ndir.create(\"/root/autodl-tmp/biomni_v20260208_pred_results/doubao-seed-1-8/\", recursive = TRUE, showWarnings = FALSE)\n\n# Read unique Series list\nseries_list <- readLines(\"/root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/cds_db_retrieval/unique_series_list.txt\")\n\n# Initialize results list\nresults <- list()\n\n# Define valid microarray platforms\nvalid_platforms <- c(\"Affymetrix\", \"Agilent\", \"Illumina\")\n\n# Process each Series\nfor (gse_id in series_list) {\n  cat(\"\\nProcessing\", gse_id, \"...\\n\")\n  \n  # Download Series matrix files (GSEMatrix=TRUE)\n  gse <- getGEO(gse_id, GSEMatrix = TRUE, destdir = \"/root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/cds_db_retrieval/\")\n  \n  # Get the ExpressionSet object\n  eset <- gse[[1]]\n  \n  # Get platform accession\n  platform_acc <- annotation(eset)\n  \n  # Download platform file\n  gpl_file <- file.path(\"/root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/cds_db_retrieval/\", paste0(platform_acc, \".soft.gz\"))\n  if (!file.exists(gpl_file)) {\n    download.file(paste0(\"https://ftp.ncbi.nlm.nih.gov/geo/platforms/\", substr(platform_acc, 1, 4), \"nnn/\", platform_acc, \"/soft/\", platform_acc, \".soft.gz\"), gpl_file)\n  }\n  \n  # Read platform title from GPL file\n  platform_title <- system(paste0(\"zgrep '^!Platform_title' \", gpl_file, \" | cut -d ' ' -f 2-\"), intern = TRUE)\n  cat(\"Platform:\", platform_title, \"\\n\")\n  \n  # Get series description from the series matrix file\n  series_desc <- system(paste0(\"zgrep '^!Series_description' /root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/cds_db_retrieval/\", gse_id, \"_series_matrix.txt.gz | cut -d ' ' -f 2-\"), intern = TRUE)\n  cat(\"Series description:\", substr(series_desc, 1, 100), \"...\\n\")\n  \n  # Check if it's bulk transcriptome (not single-cell)\n  is_bulk <- !grepl(\"single.?cell|scRNA-seq|scRNAseq\", series_desc, ignore.case = TRUE) &&\n             !grepl(\"single.?cell|scRNA-seq|scRNAseq\", platform_title, ignore.case = TRUE)\n  cat(\"Is bulk:\", is_bulk, \"\\n\")\n  \n  # Check platform validity (if microarray)\n  is_valid_platform <- any(grepl(paste(valid_platforms, collapse = \"|\"), platform_title, ignore.case = TRUE))\n  # If it's RNA-seq, platform validity doesn't apply\n  if (grepl(\"RNA-seq|RNAseq\", platform_title, ignore.case = TRUE)) {\n    is_valid_platform <- TRUE\n  }\n  cat(\"Valid platform:\", is_valid_platform, \"\\n\")\n  \n  # Get sample characteristics from the series matrix\n  sample_chars <- pData(eset)$characteristics_ch1\n  # Look for keywords indicating pre/post treatment\n  has_paired <- any(grepl(\"pre.?treatment|baseline|post.?treatment|after.?treatment\", sample_chars, ignore.case = TRUE)) ||\n                any(grepl(\"pre.?treatment|baseline|post.?treatment|after.?treatment\", series_desc, ignore.case = TRUE))\n  cat(\"Has paired pre/post samples:\", has_paired, \"\\n\")\n  \n  # Check for combination therapy\n  has_combination <- grepl(\"surgery|radiation|radiotherapy|chemoradiation\", series_desc, ignore.case = TRUE) ||\n                     any(grepl(\"surgery|radiation|radiotherapy|chemoradiation\", sample_chars, ignore.case = TRUE))\n  cat(\"Has combination therapy:\", has_combination, \"\\n\")\n  \n  # Determine if accepted (all criteria met)\n  accepted <- is_bulk && is_valid_platform && has_paired && !has_combination\n  cat(\"Accepted:\", accepted, \"\\n\")\n  \n  # Add to results\n  results[[gse_id]] <- list(Series = gse_id, Accepted = accepted)\n}\n\n# Convert to JSON array\nresults_df <- bind_rows(results)\nresults_json <- toJSON(results_df, auto_unbox = TRUE)\ncat(\"\\nFinal results JSON:\\n\", results_json, \"\\n\")\n\n# Save to file\nwrite(results_json, \"/root/autodl-tmp/biomni_v20260208_pred_results/doubao-seed-1-8/cds_db_retrieval.json\")\ncat(\"\\nResults saved to output file.\\n\")\n</execute>"
