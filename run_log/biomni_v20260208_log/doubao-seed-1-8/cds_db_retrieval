            "content": "<execute> #!R\nlibrary(GEOquery)\nlibrary(tidyverse)\n\n# Create output directory if not exists\ndir.create(\"/root/autodl-tmp/biomni_v20260208_pred_results/doubao-seed-1-8/\", recursive = TRUE, showWarnings = FALSE)\n\n# Read unique Series list\nseries_list <- readLines(\"/root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/cds_db_retrieval/unique_series_list.txt\")\n\n# Initialize results list\nresults <- list()\n\n# Define valid microarray platforms\nvalid_platforms <- c(\"Affymetrix\", \"Agilent\", \"Illumina\")\n\n# Process each Series\nfor (gse_id in series_list) {\n  cat(\"\\nProcessing\", gse_id, \"...\\n\")\n  \n  # Download Series data with GSEMatrix=FALSE to get full GSE object\n  gse <- getGEO(gse_id, GSEMatrix = FALSE, destdir = \"/root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/cds_db_retrieval/\")\n  \n  # Extract platform accession from GSE object\n  platform_acc <- gse@gsms[[1]]@platform_id\n  # Get platform data\n  gpl <- getGEO(platform_acc, destdir = \"/root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/cds_db_retrieval/\")\n  # Extract platform title from GPL header\n  platform_title <- gpl@header$title\n  cat(\"Platform:\", platform_title, \"\\n\")\n  \n  # Check if it's bulk transcriptome (not single-cell)\n  # Check in series title, description, and platform title\n  is_bulk <- !grepl(\"single.?cell|scRNA-seq|scRNAseq\", gse@title, ignore.case = TRUE) &&\n             !grepl(\"single.?cell|scRNA-seq|scRNAseq\", gse@description, ignore.case = TRUE) &&\n             !grepl(\"single.?cell|scRNA-seq|scRNAseq\", platform_title, ignore.case = TRUE)\n  cat(\"Is bulk:\", is_bulk, \"\\n\")\n  \n  # Check platform validity (if microarray)\n  is_valid_platform <- any(grepl(paste(valid_platforms, collapse = \"|\"), platform_title, ignore.case = TRUE))\n  # If it's RNA-seq, platform validity doesn't apply\n  if (grepl(\"RNA-seq|RNAseq\", platform_title, ignore.case = TRUE)) {\n    is_valid_platform <- TRUE\n  }\n  cat(\"Valid platform:\", is_valid_platform, \"\\n\")\n  \n  # Check for paired pre/post samples\n  # Extract sample characteristics\n  sample_chars <- lapply(gse@gsms, function(gsm) gsm@characteristics_ch1)\n  # Flatten to vector\n  all_chars <- unlist(sample_chars)\n  # Look for keywords indicating pre/post treatment\n  has_paired <- any(grepl(\"pre.?treatment|baseline|post.?treatment|after.?treatment\", all_chars, ignore.case = TRUE)) ||\n                any(grepl(\"pre.?treatment|baseline|post.?treatment|after.?treatment\", gse@title, ignore.case = TRUE)) ||\n                any(grepl(\"pre.?treatment|baseline|post.?treatment|after.?treatment\", gse@description, ignore.case = TRUE))\n  cat(\"Has paired pre/post samples:\", has_paired, \"\\n\")\n  \n  # Check for combination therapy\n  has_combination <- grepl(\"surgery|radiation|radiotherapy|chemoradiation\", gse@description, ignore.case = TRUE) ||\n                     any(grepl(\"surgery|radiation|radiotherapy|chemoradiation\", all_chars, ignore.case = TRUE))\n  cat(\"Has combination therapy:\", has_combination, \"\\n\")\n  \n  # Determine if accepted (all criteria met)\n  accepted <- is_bulk && is_valid_platform && has_paired && !has_combination\n  cat(\"Accepted:\", accepted, \"\\n\")\n  \n  # Add to results\n  results[[gse_id]] <- list(Series = gse_id, Accepted = accepted)\n}\n\n# Convert to JSON array\nresults_df <- bind_rows(results)\nresults_json <- toJSON(results_df, auto_unbox = TRUE)\ncat(\"\\nFinal results JSON:\\n\", results_json, \"\\n\")\n\n# Save to file\nwrite(results_json, \"/root/autodl-tmp/biomni_v20260208_pred_results/doubao-seed-1-8/cds_db_retrieval.json\")\ncat(\"\\nResults saved to output file.\\n\")\n</execute>"
