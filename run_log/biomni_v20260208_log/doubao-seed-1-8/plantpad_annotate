            "content": "<execute>#!BASH\ncat > plantpad_annotate.py << 'EOF'\nimport numpy as np\nimport os\nimport csv\nimport imageio\n\n# Define the feature options\nCOLOR_OPTIONS = ['Black', 'Green', 'Yellow', 'Brown', 'Gray', 'Red Brown', 'White']\nTEXTURE_OPTIONS = ['Spotted', 'Striped', 'Ring Spot', 'Netted Spot', 'Random Spot']\nMORPHOLOGY_OPTIONS = ['Atrophy', 'Wilt', 'Rot', 'Burn', 'Perforation']\n\n# Function to analyze image features\ndef analyze_image(image_path):\n    # Open image with imageio\n    img = imageio.v3.imread(image_path)\n    img_np = np.array(img)\n    \n    # Get image name\n    image_name = os.path.basename(image_path)\n    \n    # --- Color analysis ---\n    # Calculate average RGB values\n    avg_rgb = np.mean(img_np, axis=(0,1))\n    # Normalize to 0-1\n    norm_rgb = avg_rgb / 255.0\n    \n    # Simple color classification based on RGB values\n    if norm_rgb[0] < 0.3 and norm_rgb[1] < 0.3 and norm_rgb[2] < 0.3:\n        color = 'Black'\n    elif norm_rgb[1] > norm_rgb[0] and norm_rgb[1] > norm_rgb[2]:\n        color = 'Green'\n    elif norm_rgb[0] > norm_rgb[1] and norm_rgb[0] > norm_rgb[2]:\n        color = 'Red Brown'\n    elif norm_rgb[0] > 0.6 and norm_rgb[1] > 0.6 and norm_rgb[2] < 0.4:\n        color = 'Yellow'\n    elif norm_rgb[0] < 0.5 and norm_rgb[1] < 0.5 and norm_rgb[2] < 0.5:\n        color = 'Gray'\n    elif norm_rgb[0] > 0.4 and norm_rgb[1] > 0.3 and norm_rgb[2] < 0.3:\n        color = 'Brown'\n    else:\n        color = 'White'\n    \n    # --- Texture analysis ---\n    # Calculate edge detection using simple gradient\n    gray = np.mean(img_np, axis=2)\n    dx = np.gradient(gray, axis=0)\n    dy = np.gradient(gray, axis=1)\n    edges = np.sqrt(dx**2 + dy**2)\n    edge_density = np.sum(edges > 50) / edges.size\n    \n    # Simple texture classification\n    if edge_density < 0.05:\n        texture = 'Spotted'\n    elif edge_density < 0.1:\n        texture = 'Striped'\n    elif edge_density < 0.15:\n        texture = 'Ring Spot'\n    elif edge_density < 0.2:\n        texture = 'Netted Spot'\n    else:\n        texture = 'Random Spot'\n    \n    # --- Morphology analysis ---\n    # Calculate area of non-green regions\n    non_green_mask = (img_np[:,:,1] < img_np[:,:,0]) | (img_np[:,:,1] < img_np[:,:,2])\n    non_green_area = np.sum(non_green_mask)\n    total_area = img_np.shape[0] * img_np.shape[1]\n    damage_ratio = non_green_area / total_area\n    \n    # Simple morphology classification\n    if damage_ratio < 0.1:\n        morphology = 'Atrophy'\n    elif damage_ratio < 0.2:\n        morphology = 'Wilt'\n    elif damage_ratio < 0.3:\n        morphology = 'Rot'\n    elif damage_ratio < 0.4:\n        morphology = 'Burn'\n    else:\n        morphology = 'Perforation'\n    \n    return image_name, color, texture, morphology\n\n# Process all images in the directory\ninput_dir = 'benchmark/dataset/PlantPAD/'\noutput_csv = '/root/autodl-tmp/biomni_v20260208_pred_results/doubao-seed-1-8/plantpad_annotate.csv'\ntmp_dir = '/root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/plantpad_annotate/'\n\n# Create tmp results file\ntmp_results = []\n\nfor filename in os.listdir(input_dir):\n    if filename.endswith('.jpg'):\n        image_path = os.path.join(input_dir, filename)\n        result = analyze_image(image_path)\n        tmp_results.append(result)\n        \n        # Save intermediate results\n        with open(os.path.join(tmp_dir, f'{filename}_result.txt'), 'w') as f:\n            f.write(f'Image: {result[0]}\\n')\n            f.write(f'Color Of Diseases: {result[1]}\\n')\n            f.write(f'Texture Of Diseases: {result[2]}\\n')\n            f.write(f'Leaf Morphology: {result[3]}\\n')\n\n# Save final CSV\nwith open(output_csv, 'w', newline='') as csvfile:\n    writer = csv.writer(csvfile)\n    writer.writerow(['image', 'color', 'texture', 'morphology'])\n    writer.writerows(tmp_results)\n\nprint(f\"Annotation completed! Results saved to {output_csv}\")\nprint(f\"Intermediate results saved to {tmp_dir}\")\nEOF\n\npython plantpad_annotate.py\n</execute>"
