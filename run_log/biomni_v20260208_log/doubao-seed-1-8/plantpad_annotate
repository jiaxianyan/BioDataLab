     texture = 'Netted Spot'\n    else:\n        texture = 'Random Spot'\n    \n    # --- Morphology analysis ---\n    # Calculate aspect ratio and area of non-green regions\n    non_green_mask = (img_np[:,:,1] < img_np[:,:,0]) | (img_np[:,:,1] < img_np[:,:,2])\n    non_green_area = np.sum(non_green_mask)\n    total_area = img_np.shape[0] * img_np.shape[1]\n    damage_ratio = non_green_area / total_area\n    \n    # Simple morphology classification\n    if damage_ratio < 0.1:\n        morphology = 'Atrophy'\n    elif damage_ratio < 0.2:\n        morphology = 'Wilt'\n    elif damage_ratio < 0.3:\n        morphology = 'Rot'\n    elif damage_ratio < 0.4:\n        morphology = 'Burn'\n    else:\n        morphology = 'Perforation'\n    \n    return image_name, color, texture, morphology\n\n# Process all images in the directory\ninput_dir = 'benchmark/dataset/PlantPAD/'\noutput_csv = '/root/autodl-tmp/biomni_v20260208_pred_results/doubao-seed-1-8/plantpad_annotate.csv'\ntmp_dir = '/root/autodl-tmp/biomni_v20260208_tmp/doubao-seed-1-8/plantpad_annotate/'\n\n# Create tmp results file\ntmp_results = []\n\nfor filename in os.listdir(input_dir):\n    if filename.endswith('.jpg'):\n        image_path = os.path.join(input_dir, filename)\n        result = analyze_image(image_path)\n        tmp_results.append(result)\n        \n        # Save intermediate results\n        with open(os.path.join(tmp_dir, f'{filename}_result.txt'), 'w') as f:\n            f.write(f'Image: {result[0]}\\n')\n            f.write(f'Color Of Diseases: {result[1]}\\n')\n            f.write(f'Texture Of Diseases: {result[2]}\\n')\n            f.write(f'Leaf Morphology: {result[3]}\\n')\n\n# Save final CSV\nwith open(output_csv, 'w', newline='') as csvfile:\n    writer = csv.writer(csvfile)\n    writer.writerow(['image', 'color', 'texture', 'morphology'])\n    writer.writerows(tmp_results)\n\nprint(f\"Annotation completed! Results saved to {output_csv}\")\nprint(f\"Intermediate results saved to {tmp_dir}\")\n</execute>"
