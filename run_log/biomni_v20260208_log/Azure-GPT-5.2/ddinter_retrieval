ite(json.dumps(date_params, indent=2))\n\nprint(\"\\nUPDATED PLAN:\")\nfor i,s in enumerate(checklist,1):\n    print(f\"{i}. [{'\u2713' if i<=1 else ' '}] {s}\")\n\n# Step 2: ESearch\nesearch_params = {\"db\": \"pubmed\", \"term\": query, \"retmax\": 100000, \"retmode\": \"json\", **date_params}\nesearch_url = BASE + \"esearch.fcgi?\" + urlencode(esearch_params)\nesearch_json = json.loads(get_text(esearch_url, timeout=60))\npmids = esearch_json[\"esearchresult\"].get(\"idlist\", [])\ncount = int(esearch_json[\"esearchresult\"].get(\"count\", \"0\"))\n\nwith open(os.path.join(tmp_dir, \"esearch.json\"), \"w\") as f:\n    json.dump(esearch_json, f, indent=2)\n\nprint(f\"\\nSTEP 2: count={count}, retrieved={len(pmids)} PMIDs\")\nprint(\"\\nUPDATED PLAN:\")\nfor i,s in enumerate(checklist,1):\n    print(f\"{i}. [{'\u2713' if i<=2 else ' '}] {s}\")\n\n# Step 3: EFetch + parse\narticles = {}\nbatch_size = 200\nfor i in range(0, len(pmids), batch_size):\n    batch = pmids[i:i+batch_size]\n    efetch_params = {\"db\": \"pubmed\", \"id\": \",\".join(batch), \"retmode\": \"xml\"}\n    efetch_url = BASE + \"efetch.fcgi?\" + urlencode(efetch_params)\n    xml_text = get_text(efetch_url, timeout=120)\n\n    xml_path = os.path.join(tmp_dir, f\"efetch_{i//batch_size:04d}.xml\")\n    with open(xml_path, \"w\") as f:\n        f.write(xml_text)\n\n    soup = BeautifulSoup(xml_text, \"xml\")\n    for art in soup.find_all(\"PubmedArticle\"):\n        pmid_tag = art.find(\"PMID\")\n        if not pmid_tag:\n            continue\n        pmid = pmid_tag.get_text(strip=True)\n        title_tag = art.find(\"ArticleTitle\")\n        title = title_tag.get_text(\" \", strip=True) if title_tag else \"\"\n        abst_nodes = art.find_all(\"AbstractText\")\n        abstract = \" \".join(n.get_text(\" \", strip=True) for n in abst_nodes) if abst_nodes else \"\"\n        articles[pmid] = {\"title\": title, \"abstract\": abstract}\n\nwith open(os.path.join(tmp_dir, \"parsed_articles.json\"), \"w\")