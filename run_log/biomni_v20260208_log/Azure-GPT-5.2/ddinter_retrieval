omplete.\")\nprint(\"\\nUPDATED PLAN:\")\nshow_plan(1)\n\n# HTTP helper\nBASE = \"https://eutils.ncbi.nlm.nih.gov/entrez/eutils/\"\ndef http_get(endpoint, params, timeout=60):\n    url = BASE + endpoint + \"?\" + urlencode(params)\n    with urlopen(url, timeout=timeout) as resp:\n        return resp.read().decode(\"utf-8\")\n\n# ========== Step 2: ESearch ==========\nesearch_params = {\"db\": \"pubmed\", \"term\": query, \"retmax\": 100000, \"retmode\": \"json\", **date_params}\nesearch_text = http_get(\"esearch.fcgi\", esearch_params, timeout=60)\nesearch_json = json.loads(esearch_text)\n\npmids = esearch_json[\"esearchresult\"].get(\"idlist\", [])\ncount = int(esearch_json[\"esearchresult\"].get(\"count\", \"0\"))\n\nwith open(os.path.join(tmp_dir, \"esearch.json\"), \"w\") as f:\n    json.dump(esearch_json, f, indent=2)\n\nprint(f\"\\nSTEP 2 complete: count={count}, retrieved={len(pmids)} PMIDs.\")\nprint(\"\\nUPDATED PLAN:\")\nshow_plan(2)\n\n# ========== Step 3: EFetch + parse ==========\ndef efetch_xml(id_batch):\n    params = {\"db\": \"pubmed\", \"id\": \",\".join(id_batch), \"retmode\": \"xml\"}\n    return http_get(\"efetch.fcgi\", params, timeout=120)\n\narticles = {}\nbatch_size = 200\nfor i in range(0, len(pmids), batch_size):\n    batch = pmids[i:i+batch_size]\n    xml_text = efetch_xml(batch)\n\n    xml_path = os.path.join(tmp_dir, f\"efetch_{i//batch_size:04d}.xml\")\n    with open(xml_path, \"w\") as f:\n        f.write(xml_text)\n\n    soup = BeautifulSoup(xml_text, \"xml\")\n    for art in soup.find_all(\"PubmedArticle\"):\n        pmid_tag = art.find(\"PMID\")\n        if not pmid_tag:\n            continue\n        pmid = pmid_tag.get_text(strip=True)\n        title_tag = art.find(\"ArticleTitle\")\n        title = title_tag.get_text(\" \", strip=True) if title_tag else \"\"\n        abst_nodes = art.find_all(\"AbstractText\")\n        abstract = \" \".join(n.get_text(\" \", strip=True) for n in abst_nodes) if abst_nodes else \"\"\n        articles[pmid] = {\"title\": title, \"abstract\": abstract}\n\nwith open(os.path.join(tmp_dir, \"parsed_articles.json\"), \"w\") as f:\n    json.dump(articles, f, indent=2)\n\nprint(f\"\\nSTEP 3 complete: parsed={len(articles)} articles.\")\nprint(\"\\nUPDATED PLAN:\")\nshow_plan(3)\n\n# ========== Step 4: Screening ==========\nother_drug_markers = [\n    \"warfarin\",\"theophylline\",\"tizanidine\",\"cyclosporine\",\"clozapine\",\"duloxetine\",\"methotrexate\",\n    \"metronidazole\",\"rifampin\",\"rifampicin\",\"probenecid\",\"phenytoin\",\"digoxin\",\n    \"clopidogrel\",\"apixaban\",\"rivaroxaban\",\"dabigatran\",\"amiodarone\",\"fluconazole\",\n    \"azithromycin\",\"clarithromycin\",\"linezolid\",\"vancomycin\",\"gentamicin\",\n    \"sucralfate\",\"antacid\",\"iron\",\"zinc\",\"calcium\",\"magnesium\"\n]\nexclude_terms = [\n    \"food-drug\",\"food drug\",\"diet\",\"meal\",\"nutrition\",\"grapefruit\",\n    \"pharmacogen\",\"pharmacogenetic\",\"genetic\",\"genotype\",\"polymorphism\",\n    \" gene\",\" genes\",\"cyp2\",\"cyp3\",\"cyp1a2\"\n]\nnon_ddi_focus_terms = [\n    \"antimicrobial susceptibility\",\"minimum inhibitory concentration\",\" mic\",\n    \"biofilm\",\"resistance\",\"resistant\",\"synergistic activity\",\"checkerboard\",\"time-kill\"\n]\n\ndef is_accepted(title, abstract):\n    text = (title + \" \" + abstract).lower()\n    if \"ciprofloxacin\" not in text:\n        return False\n    if any(t in text for t in exclude_terms):\n        return False\n    ddi_signal = any(k in text for k in [\n        \"drug-drug\",\"drug drug\",\"interaction\",\"concomitant\",\"coadministr\",\"co-admin\",\n        \"pharmacokinetic\",\"auc\",\"cmax\",\"clearance\",\"inhibit\",\"induc\",\"exposure\",\"qt\"\n    ])\n    if not ddi_signal:\n        return False\n    if any(t in text for t in non_ddi_focus_terms) and not any(k in text for k in [\n        \"pharmacokinetic\",\"auc\",\"cmax\",\"clearance\",\"toxicity\",\"bleeding\",\"serum concentration\",\"plasma concentration\"\n    ]):\n        return False\n    other = any(m in text for m in other_drug_markers)\n    if not other and re.search(r\"ciprofloxacin (and|with) [a-z0-9\\-]{3,}\", text):\n        other = True\n    return other\n\nresults = []\nfor pmid in pmids:\n    meta = articles.get(pmid, {\"title\": \"\", \"abstract\": \"\"})\n    results.append({\"PMID\": str(pmid), \"Accepted\": bool(is_accepted(meta[\"title\"], meta[\"abstract\"]))})\n\nn_acc = sum(1 for x in results if x[\"Accepted\"])\nwith open(os.path.join(tmp_dir, \"screening_results.json\"), \"w\") as f:\n    json.dump(results, f, indent=2)\n\nprint(f\"\\nSTEP 4 complete: Accepted={n_acc}/{len(results)}\")\nprint(\"Sample Accepted PMIDs:\", [x[\"PMID\"] for x in results if x[\"Accepted\"]][:10])\n\nprint(\"\\nUPDATED PLAN:\")\nshow_plan(4)\n\n# ========== Step 5: Write final ==========\nwith open(out_path, \"w\") as f:\n    json.dump(results, f, indent=2)\n\nsummary = {\n    \"query\": query,\n    \"date_params\": date_params,\n    \"initial_count\": count,\n    \"retrieved_pmids\": len(pmids),\n    \"accepted\": n_acc,\n    \"timestamp\": datetime.utcnow().isoformat() + \"Z\"\n}\nwith open(os.path.join(tmp_dir, \"run_summary.json\"), \"w\") as f:\n    json.dump(summary, f, indent=2)\n\nprint(f\"\\nSTEP 5 complete: wrote output JSON to {out_path}\")\nprint(\"\\nUPDATED PLAN:\")\nshow_plan(5)\nprint(\"\\nRun summary:\", summary)\n</execute>"
