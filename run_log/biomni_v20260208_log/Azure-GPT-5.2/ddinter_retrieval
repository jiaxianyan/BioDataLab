date=$MINDATE\" --data-urlencode \"maxdate=$MAXDATE\" --data-urlencode \"datetype=pdat\" \\\\\n  > \"$TMP/esearch.json\"\n\npython3 - <<'PY'\nimport json, pathlib\ntmp = pathlib.Path({json.dumps(tmp_dir)})\ndata = json.loads((tmp/\"esearch.json\").read_text())\npmids = data[\"esearchresult\"].get(\"idlist\", [])\n(tmp/\"pmids.txt\").write_text(\"\\\\n\".join(pmids))\nprint(\"PMIDs:\", len(pmids))\nPY\n\n# Step 3: EFetch XML (batch)\nEF_URL=\"https://eutils.ncbi.nlm.nih.gov/entrez/eutils/efetch.fcgi\"\nsplit -l 200 \"$TMP/pmids.txt\" \"$TMP/pmids_chunk_\"\nfor f in \"$TMP\"/pmids_chunk_*; do\n  IDS=$(paste -sd, \"$f\")\n  out=\"$TMP/efetch_$(basename \"$f\").xml\"\n  curl -sG \"$EF_URL\" --data-urlencode \"db=pubmed\" --data-urlencode \"id=$IDS\" --data-urlencode \"retmode=xml\" > \"$out\"\ndone\n\"\"\"\nscript_path = os.path.join(tmp_dir, \"run_pubmed.sh\")\nwith open(script_path, \"w\") as f:\n    f.write(bash_script)\nos.chmod(script_path, 0o755)\n\nprint(\"\\nSTEP 1 complete (query + script written).\")\nprint(\"\\nUPDATED PLAN:\")\nfor i,s in enumerate(plan,1):\n    print(f\"{i}. [{'\u2713' if i<=1 else ' '}] {s}\")\n\nprint(\"\\nRunning bash retrieval pipeline...\")\nres = subprocess.run([\"bash\", script_path], capture_output=True, text=True)\nprint(\"STDOUT:\\n\", res.stdout)\nprint(\"STDERR:\\n\", res.stderr)\nif res.returncode != 0:\n    raise SystemExit(f\"Bash pipeline failed with code {res.returncode}\")\n\nprint(\"\\nUPDATED PLAN:\")\nfor i,s in enumerate(plan,1):\n    print(f\"{i}. [{'\u2713' if i<=3 else ' '}] {s}\")\n\n# Step 4-5: parse efetch xml files + screen + write output\nimport glob\nfrom bs4 import BeautifulSoup\nimport re\nfrom datetime import datetime\n\npmids = open(os.path.join(tmp_dir,\"pmids.txt\")).read().strip().splitlines()\narticles = {}\nfor xmlf in sorted(glob.glob(os.path.join(tmp_dir,\"efetch_pmids_chunk_*.xml\"))):\n    xml_text = open(xmlf).read()\n    soup = BeautifulSoup(xml_text, \"xml\")\n    for art in soup.find_all(\"Pub