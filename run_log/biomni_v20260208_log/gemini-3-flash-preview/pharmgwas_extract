xt' in data.get('_links', {}):\n        next_url = data['_links']['next']['href']\n        response = requests.get(next_url, headers=headers)\n        if response.status_code != 200:\n            break\n        data = response.json()\n        extract_2025_studies(data)\n        page_count += 1\n        if page_count > 50: # Safety break\n            break\n            \n    return all_results\n\n# Execute search\nresults = get_all_studies(\"Atrial Fibrillation\")\nprint(f\"Extracted {len(results)} studies from 2025.\")\n\n# Create DataFrame\nif not results:\n    # If no results found, create an empty dataframe with required headers\n    df = pd.DataFrame(columns=['PMID', 'Accession'])\nelse:\n    df = pd.DataFrame(results)\n\n# Ensure no duplicates\ndf = df.drop_duplicates()\n\n# Save to the specific output path\noutput_path = \"/root/autodl-tmp/biomni_v20260208_pred_results/gemini-3-flash-preview/pharmgwas_extract.csv\"\ndf.to_csv(output_path, index=False)\nprint(f\"Results saved to {output_path}\")\nprint(df)</execute>"
