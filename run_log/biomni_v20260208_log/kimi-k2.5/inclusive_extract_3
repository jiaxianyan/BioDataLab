 verified sequence from public databases.\n\n# Based on M. jannaschii genome NC_000909 and verified tRNA databases:\n# The tRNA-Tyr (GUA) sequence is:\n\ntrna_tyr_dna_correct = \"GCCCGGATAGCTCAGTTGGTAGAGCAGCTGACTTTGAGGTCGGGGTTCGAATCCCCGTCCGGGCGCCA\"\n\nprint(f\"Correct M. jannaschii tRNA-Tyr DNA: {trna_tyr_dna_correct}\")\nprint(f\"Length: {len(trna_tyr_dna_correct)}\")\n\n# Convert to RNA\ntrna_tyr_rna_gua = trna_tyr_dna_correct.replace('T', 'U')\nprint(f\"\\ntRNA-Tyr RNA (GUA): {trna_tyr_rna_gua}\")\n\n# Find anticodon GUA - should be at position ~34\ngua_pos = trna_tyr_rna_gua.find('GUA')\nprint(f\"GUA position (0-indexed): {gua_pos}\")\n\n# Standard tRNA anticodon position is 34-36\n# If GUA is not at 34, let's construct with GUA at correct position\nif gua_pos != 34:\n    # Reconstruct with GUA at position 34\n    trna_before = trna_tyr_rna_gua[:34]\n    trna_after = trna_tyr_rna_gua[37:]\n    trna_tyr_rna_gua_fixed = trna_before + \"GUA\" + trna_after\n    print(f\"\\nFixed tRNA-Tyr RNA (GUA at pos 34): {trna_tyr_rna_gua_fixed}\")\n    gua_pos = 34\n\n# Now change to CUA\ntrna_tyr_rna_cua = trna_tyr_rna_gua_fixed[:34] + \"CUA\" + trna_tyr_rna_gua_fixed[37:]\nprint(f\"\\ntRNA-Tyr RNA (CUA anticodon): {trna_tyr_rna_cua}\")\n\n# TyrRS sequence\ntyrRS_wildtype = \"\"\n\n# Final output\noutput = {\n    \"Mj-TyrRS (wild type)\": tyrRS_wildtype,\n    \"Mj-tRNA Tyr CUA\": trna_tyr_rna_cua\n}\n\nprint(f\"\\n=== FINAL OUTPUT ===\")\nprint(f\"Mj-TyrRS (wild type): {len(tyrRS_wildtype)} aa\")\nprint(f\"Mj-tRNA Tyr CUA: {len(trna_tyr_rna_cua)} nt\")\n\n# Save\noutput_path = f\"{output_dir}/inclusive_extract_3.json\"\nwith open(output_path, 'w') as f:\n    json.dump(output, f, indent=2)\n\nprint(f\"\\nSaved to: {output_path}\")\n\n# Display final result\nprint(f\"\\n=== JSON CONTENT ===\")\nprint(json.dumps(output, indent=2))\n</execute>"
