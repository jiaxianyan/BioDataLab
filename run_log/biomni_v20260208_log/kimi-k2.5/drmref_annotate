ate(genes):\n    # Get expression values for this gene\n    res_vals = resistant_expr[:, i]\n    sen_vals = sensitive_expr[:, i]\n    \n    # Calculate means for log2 fold change\n    res_mean = np.mean(res_vals)\n    sen_mean = np.mean(sen_vals)\n    \n    # Avoid division by zero and log of zero\n    res_mean = np.maximum(res_mean, 1e-10)\n    sen_mean = np.maximum(sen_mean, 1e-10)\n    \n    # Calculate log2 fold change (resistant vs sensitive)\n    log2fc = np.log2(res_mean / sen_mean)\n    \n    # Perform Wilcoxon rank-sum test\n    try:\n        statistic, pvalue = stats.ranksums(res_vals, sen_vals)\n    except:\n        pvalue = 1.0\n    \n    results.append({\n        'gene': gene,\n        'pvalue': pvalue,\n        'log2fc': log2fc,\n        'resistant_mean': res_mean,\n        'sensitive_mean': sen_mean\n    })\n    \n    if (i + 1) % 500 == 0:\n        print(f\"  Processed {i + 1}/{len(genes)} genes\")\n\nprint(f\"\\nCompleted differential expression analysis for {len(results)} genes\")\n\n# Convert to DataFrame\nresults_df = pd.DataFrame(results)\nprint(f\"\\nResults DataFrame shape: {results_df.shape}\")\n\n# Calculate adjusted p-values using Benjamini-Hochberg correction\nreject, pvalues_adj, _, _ = multipletests(results_df['pvalue'], method='fdr_bh')\nresults_df['padj'] = pvalues_adj\n\nprint(\"\\nSummary of differential expression results:\")\nprint(f\"\\nTotal genes tested: {len(results_df)}\")\nprint(f\"Genes with p < 0.05: {(results_df['pvalue'] < 0.05).sum()}\")\nprint(f\"Genes with padj < 0.05: {(results_df['padj'] < 0.05).sum()}\")\nprint(f\"Genes with log2fc > 1.0: {(results_df['log2fc'] > 1.0).sum()}\")\nprint(f\"Genes with log2fc < -1.0: {(results_df['log2fc'] < -1.0).sum()}\")\n\n# Filter for significant upregulated genes in resistant group\n# Criteria: adjusted p-value < 0.05 AND log2 fold change > 1.0\nsignificant_genes = results_df[(results_df['padj'] < 0.05) & (results_df['log2fc'] > 1.0)].copy()\n\nprint(f\"\\nSignificant upregulated genes (padj < 0.05, log2fc > 1.0): {len(significant_genes)}\")\n\n# Sort by log2fc descending\nsignificant_genes = significant_genes.sort_values('log2fc', ascending=False)\n\nprint(f\"\\nTop 10 significant genes:\\n{significant_genes[['gene', 'pvalue', 'padj', 'log2fc']].head(10)}\")\n\n# Create intermediate directory\nintermediate_dir = '/root/autodl-tmp/biomni_v20260208_tmp/kimi-k2.5/drmref_annotate'\nos.makedirs(intermediate_dir, exist_ok=True)\nprint(f\"\\nCreated intermediate directory: {intermediate_dir}\")\n\n# Save full results to intermediate directory\nfull_results_path = os.path.join(intermediate_dir, 'full_de_results.csv')\nresults_df.to_csv(full_results_path, index=False)\nprint(f\"Saved full DE results to: {full_results_path}\")\n\n# Save significant results to intermediate directory\nsignificant_results_path = os.path.join(intermediate_dir, 'significant_genes.csv')\nsignificant_genes.to_csv(significant_results_path, index=False)\nprint(f\"Saved significant genes results to: {significant_results_path}\")\n\n# Save the gene names to the final output file\noutput_dir = '/root/autodl-tmp/biomni_v20260208_pred_results/kimi-k2.5'\nos.makedirs(output_dir, exist_ok=True)\n\noutput_path = os.path.join(output_dir, 'drmref_annotate.csv')\ngene_list = significant_genes[['gene']].copy()\ngene_list.columns = ['gene']\ngene_list.to_csv(output_path, index=False)\n\nprint(f\"\\nSaved final gene list ({len(gene_list)} genes) to: {output_path}\")\nprint(f\"\\nFirst 20 genes:\\n{gene_list.head(20)}\")\n</execute>"
