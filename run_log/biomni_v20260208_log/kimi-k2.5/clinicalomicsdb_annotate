alize results list\nresults = []\n\n# Process each gene\nfor i, gene in enumerate(expr_data.index):\n    # Get expression values for responders and non-responders\n    responder_values = expr_data.iloc[i, responder_indices].values\n    non_responder_values = expr_data.iloc[i, non_responder_indices].values\n    \n    # Wilcoxon rank-sum test (Mann-Whitney U test)\n    statistic, p_value = stats.mannwhitneyu(responder_values, non_responder_values, alternative='two-sided')\n    \n    # Calculate AUROC\n    # Combine data: 1 for responders, 0 for non-responders\n    all_values = np.concatenate([responder_values, non_responder_values])\n    labels = np.concatenate([np.ones(len(responder_values)), np.zeros(len(non_responder_values))])\n    \n    auroc = roc_auc_score(labels, all_values)\n    \n    results.append({\n        'gene': gene,\n        'p_value': p_value,\n        'auroc': auroc\n    })\n    \n    if (i + 1) % 10 == 0:\n        print(f\"Processed {i+1}/{len(expr_data)} genes...\")\n\nprint(f\"\\nCompleted processing all {len(expr_data)} genes\")\n\n# Create results dataframe\nresults_df = pd.DataFrame(results)\nprint(f\"\\nResults dataframe shape: {results_df.shape}\")\nprint(f\"\\nFirst 5 results:\")\nprint(results_df.head())\n\n# Save intermediate results\nresults_df.to_csv(f'{tmp_dir}/intermediate_results.csv', index=False)\nprint(f\"\\nIntermediate results saved to: {tmp_dir}/intermediate_results.csv\")\n\nprint(\"\\n\" + \"=\" * 60)\nprint(\"Step 3: Saving final results\")\nprint(\"=\" * 60)\n\n# Save final results\noutput_path = f'{results_dir}/clinicalomicsdb_annotate.csv'\nresults_df.to_csv(output_path, index=False)\nprint(f\"Final results saved to: {output_path}\")\n\n# Verify output\nprint(f\"\\nOutput file contents (first 10 rows):\")\nprint(results_df.head(10))\nprint(f\"\\nTotal genes analyzed: {len(results_df)}\")\nprint(f\"Min p-value: {results_df['p_value'].min():.6f}\")\nprint(f\"Max p-value: {results_df['p_value'].max():.6f}\")\nprint(f\"Min AUROC: {results_df['auroc'].min():.4f}\")\nprint(f\"Max AUROC: {results_df['auroc'].max():.4f}\")\n</execute>"
