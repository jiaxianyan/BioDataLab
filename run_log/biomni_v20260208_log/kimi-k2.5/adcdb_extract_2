  has_efficacy = \"efficacy\" in combined_text or \"antitumor\" in combined_text or \"anti-tumor\" in combined_text or \"cytotoxicity\" in combined_text\n    has_adc = \"antibody-drug conjugate\" in combined_text or \" adc \" in combined_text or \"vedotin\" in combined_text or \"immunoconjugate\" in combined_text\n    \n    # Accepted if it investigates pharmacology, mechanism, or efficacy with ADC context\n    accepted = has_pharmacology or has_mechanism or (has_efficacy and has_adc)\n    \n    results.append({\"PMID\": pmid, \"Accepted\": accepted})\n    print(f\"PMID {pmid}: Pharma={has_pharmacology}, Mech={has_mechanism}, Eff={has_efficacy}, ADC={has_adc} => Accepted={accepted}\")\n\n# Save results\nwith open(result_file, \"w\") as f:\n    json.dump(results, f, indent=2)\n\nprint(f\"\\nResults saved to {result_file}\")\nprint(f\"Total: {len(results)}, Accepted: {sum(r['Accepted'] for r in results)}\")\n</execute>"
