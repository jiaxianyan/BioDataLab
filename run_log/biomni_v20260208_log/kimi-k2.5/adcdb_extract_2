   has_mechanism = any(kw in full_text for kw in ['mechanism', 'binding', 'inhibition', 'signaling', 'internalization', 'payload', 'mmae', 'microtubule', 'apoptosis'])\n    has_efficacy = any(kw in full_text for kw in ['efficacy', 'antitumor', 'anti-tumor', 'response rate', 'tumor regression', 'cytotoxicity', 'cell death'])\n    has_adc = any(kw in full_text for kw in ['antibody-drug conjugate', 'adc', 'vedotin', 'immunoconjugate'])\n    \n    # Article accepted if it investigates pharmacology, mechanism, or efficacy with ADC context\n    accepted = has_pharmacology or has_mechanism or (has_efficacy and has_adc)\n    \n    print(f\"Pharmacology: {has_pharmacology} | Mechanism: {has_mechanism} | Efficacy: {has_efficacy} | ADC: {has_adc}\")\n    print(f\"ACCEPTED: {accepted}\")\n    \n    results.append({\n        \"PMID\": pmid,\n        \"Accepted\": accepted\n    })\n\nprint(f\"\\n{'='*50}\")\nprint(\"SUMMARY\")\nprint(f\"{'='*50}\")\nprint(f\"Total articles: {len(results)}\")\nprint(f\"Accepted (biological activity): {sum(1 for r in results if r['Accepted'])}\")\nprint(f\"Rejected: {sum(1 for r in results if not r['Accepted'])}\")\n</execute>"
