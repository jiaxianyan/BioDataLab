n    mechanism_terms = ['mechanism', 'binding', 'inhibition', 'signaling', 'internalization', 'mmae', 'microtubule']\n    efficacy_terms = ['efficacy', 'antitumor', 'anti-tumor', 'response rate', 'tumor regression', 'cytotoxicity']\n    adc_terms = ['antibody-drug conjugate', 'adc', 'vedotin', 'immunoconjugate']\n    \n    has_pharma = any(term in text_lower for term in pharma_terms)\n    has_mechanism = any(term in text_lower for term in mechanism_terms)\n    has_efficacy = any(term in text_lower for term in efficacy_terms)\n    has_adc = any(term in text_lower for term in adc_terms)\n    \n    # Decision: accepted if investigating pharmacology, mechanism, or efficacy with ADC context\n    is_accepted = has_pharma or has_mechanism or (has_efficacy and has_adc)\n    \n    print(f\"  Checks - Pharma: {has_pharma}, Mechanism: {has_mechanism}, Efficacy: {has_efficacy}, ADC: {has_adc}\")\n    print(f\"  => ACCEPTED: {is_accepted}\\n\")\n    \n    results.append({\"PMID\": pmid, \"Accepted\": is_accepted})\n\nprint(\"=\" * 50)\nprint(\"FINAL RESULTS\")\nprint(\"=\" * 50)\nfor r in results:\n    print(f\"PMID: {r['PMID']}, Accepted: {r['Accepted']}\")\n</execute>"
