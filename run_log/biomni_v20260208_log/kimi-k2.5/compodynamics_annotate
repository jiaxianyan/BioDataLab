ount = seq.count('G') + seq.count('C') + seq.count('g') + seq.count('c')\n        cds_sequences.append({\n            'header': header,\n            'sequence': seq,\n            'length': length,\n            'gc_count': gc_count,\n            'gc_content': gc_count / length if length > 0 else 0\n        })\n        print(f\"CDS: Length={length}, GC count={gc_count}, GC content={gc_count/length:.4f}\")\n\nprint(f\"\\nTotal CDS sequences found: {len(cds_sequences)}\")\n\n# Step 3 & 4: Calculate weighted average of GC content\nprint(\"\\n=== Step 3 & 4: Calculating weighted average GC content ===\")\ntotal_gc_nucleotides = 0\ntotal_length = 0\nfor cds in cds_sequences:\n    total_gc_nucleotides += cds['gc_count']\n    total_length += cds['length']\n\nprint(f\"\\nTotal length of all CDS: {total_length}\")\nprint(f\"Total GC nucleotides: {total_gc_nucleotides}\")\n\n# Weighted average = sum(GC_count) / sum(length)\nweighted_gc_content = total_gc_nucleotides / total_length if total_length > 0 else 0\n\nprint(f\"\\nWeighted average GC content: {total_gc_nucleotides} / {total_length} = {weighted_gc_content:.4f}\")\n\n# Step 5: Save to output file\nwith open(output_file, 'w') as f:\n    f.write(f\"{weighted_gc_content:.4f}\")\n\nprint(f\"\\nResult saved to: {output_file}\")\nprint(f\"\\nFinal result: {weighted_gc_content:.4f}\")\n</execute>"
