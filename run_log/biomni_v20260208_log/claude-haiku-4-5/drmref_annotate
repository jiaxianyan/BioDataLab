# Extract just the gene names\ngene_list_df = pd.DataFrame({\n    'gene': filtered_df['gene'].values\n})\n\n# Save to the specified location\noutput_path = '/root/autodl-tmp/biomni_v20260208_pred_results/claude-haiku-4-5/drmref_annotate.csv'\ngene_list_df.to_csv(output_path, index=False)\n\nprint(f\"\\n\u2713 Gene list saved to: {output_path}\")\nprint(f\"\\nFinal results:\")\nprint(f\"  Total significant genes: {len(gene_list_df)}\")\nprint(f\"\\nGene names:\")\nprint(gene_list_df.to_string(index=False))\n\n# Verify file was created\nif os.path.exists(output_path):\n    print(f\"\\n\u2713 File verification: Successfully created\")\n    file_size = os.path.getsize(output_path)\n    print(f\"  File size: {file_size} bytes\")\n</execute>"
